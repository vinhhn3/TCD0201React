import React from 'react';
import PropTypes from 'prop-types';

Author.propTypes = {
    author: PropTypes.array
};

Author.defaultProps = {
    author: [],
}

function Author(props) {

    const {author} = props;

    return (
        <div>
            <span>
                <small>Author: </small>
                {author.authorName}
            </span>
        </div>
    );
}

export default Author;