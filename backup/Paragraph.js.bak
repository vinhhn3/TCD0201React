import React from 'react';
import PropTypes from 'prop-types';

Paragraph.propTypes = {
    paragraph: PropTypes.array
};

Paragraph.defaultProps = {
    paragraph: []
};

function Paragraph(props) {

    const {paragraph} = props;

    return (
        <p>
            {paragraph.content}
        </p>
    );
}

export default Paragraph;